{
  "extends": "airbnb-base/legacy",
  "rules": {
    "indent": [0],
    "no-param-reassign": [0],
    "space-in-parens": [0],
    "quotes": [0],
    "key-spacing": [0],
    "comma-style": [0],
    "padded-blocks": [0],
    "func-names": [0],
    "space-before-function-paren": [0],
    "no-multi-spaces": [0],
    "no-trailing-spaces": [0],
    "{no-bitwise": [0],
    "vars-on-top": [0],
    "comma-spacing": [0],
    "keyword-spacing": [0],
    "no-plusplus": [0],
    "new-cap": [0],
    "consistent-return": [0],
    "brace-style": [0]
  }
}
